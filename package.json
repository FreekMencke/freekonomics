{
  "name": "freekonomics",
  "version": "0.1.0",
  "private": true,
  "description": "A web app that provides a comprehensive overview of your wealth and financial status",
  "keywords": [
    "wealth",
    "financial status",
    "banking",
    "finance",
    "budgeting",
    "psd2"
  ],
