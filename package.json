  },
  "scripts": {
    "build": "ng build",
    "predeploy": "ng build",
    "deploy": "aws s3 sync --delete ./dist/psd2-overview s3://freekonomics.freekmencke.com",
    "deploy:dryrun": "npm run deploy -- --dryrun",
    "lint": "ng lint",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "start": "ng serve --open",
    "test": "ng test",
    "test:ci": "ng test --watch false --browsers ChromeHeadless",
    "watch": "ng build --watch --configuration development"
  },
